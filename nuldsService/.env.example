# Nulds API endpoint (cron fetches from here every minute)
NULDS_API_URL=https://dinoparks.herokuapp.com/nudls/feed

# Database path (use same path as gamesys to share storage)
DATABASE_PATH=dino-park.db

# Optional: port if running as HTTP (default 3001)
PORT=3001

# Redis URL for dino location storage (required)
REDIS_URL=redis://localhost:6379
